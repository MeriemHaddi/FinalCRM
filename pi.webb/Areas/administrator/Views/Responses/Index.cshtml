@model IEnumerable<pi.domaine.Entities.Response>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Head_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ResponseDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Content)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ResponseDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Content)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IdResponse }) |
                @Html.ActionLink("Details", "Details", new { id = item.IdResponse }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.IdResponse })
            </td>
        </tr>
    }

</table>
